@OFF
eval -pms n "Enter n = "

if $n < 0
    jump :is_not_prime
endif

if $n == 0
    jump :is_not_prime
endif

if $n == 1
    jump :is_not_prime
endif

for div 2 "$n / 2 + 1" -t range
    if -m "$n % $div" == 0
        jump :is_not_prime
    endif

    if -m "$div * $div" > $n
        jump :is_prime
    endif
endfor

:is_prime
echoln "$n is a prime"
jump :EOF

:is_not_prime
echoln "$n is not a prime"
jump :EOF
